package day4;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class MemberVO {
	
	private int no;
	private String id;
	private String pw;
	private String name;
	private String gender;
	private String motive;
	
	public MemberVO(String id, String pw, String name, String gender, String motive) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.gender = gender;
		this.motive = motive;
	}

	
}
